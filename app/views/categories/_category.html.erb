<div id="<%= dom_id category %>">
  <tr>
    <td>- <%= category.name %></td>
    <td><%= link_to t('.edit'), edit_category_path(category) %></td>
    <td>
      <%= link_to t('.delete'), category_path(category), data: {
        turbo_method: :delete,
        turbo_confirm: t('common.confirm')
      } %>
    </td>
  </tr>
</div>
